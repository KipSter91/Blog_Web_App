<%- include('partials/header'); -%>

    <h1>Compose</h1>
    <form action="/compose" method="post">
        <label>
            <h5>Title</h5>
        </label>
        <input type="text" name="postTitle" class="titleName" id="postTitle" autocomplete="off">
        <label>
            <h5>Post</h5>
        </label>
        <textarea name="postBody" class="newContent" id="postBody" cols="30" rows="10"></textarea>
        <button type="submit" class="btn btn-outline-dark btn-md">Publish</button>
    </form>

    <%- include('partials/footer'); -%>